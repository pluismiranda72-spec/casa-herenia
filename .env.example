# Copia como .env.local y rellena los valores
# cp .env.example .env.local

NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Para cron send-survey (lectura de reservas): Project Settings > API > service_role
SUPABASE_SERVICE_ROLE_KEY=
# URL pública del sitio (para enlaces en emails de encuesta). En Vercel suele no hacer falta.
NEXT_PUBLIC_SITE_URL=
# Secreto para proteger GET /api/cron/send-survey (Vercel Cron o similar)
CRON_SECRET=
OPENAI_API_KEY=
RESEND_API_KEY=
RESEND_FROM_EMAIL=
# Email donde recibir notificaciones de nuevas reservas (si no se define, se usa RESEND_FROM_EMAIL)
RESEND_TO_EMAIL=
# Número para el botón WhatsApp en la página de gracias (ej: 34600000000 o 5351234567)
NEXT_PUBLIC_WHATSAPP_NUMBER=
# Calendarios Airbnb (recomendado: ROOM1, ROOM2, FULL)
AIRBNB_ICAL_ROOM1=
AIRBNB_ICAL_ROOM2=
AIRBNB_ICAL_FULL=
# Legacy (alternativa): AIRBNB_ICAL_URL_1, AIRBNB_ICAL_URL_2, AIRBNB_ICAL_URL_PACK
TRIPADVISOR_API_KEY=
# URL que devuelve JSON de reseñas (array con rating, text, user.name, etc.)
TRIPADVISOR_API_URL=
# Cloudinary: admin fotos habitaciones (Dashboard > Settings > Upload > Upload presets)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=